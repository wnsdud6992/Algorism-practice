num=int(input())
five=0
three=0
total=0
if num % 5==0:
    five=num/5
else:
    while total<num-5:
        total+=5
        five+=1

while num != (5*five+3*three):
    if (num-total)%3==0:
        three=(num-total)/3
    else:
        total-=5
        five-=1
        if total<0:
            total=-1
            break
if total<0:
    print(total)
else:
    print(int(five+three))
