#include<stdio.h>                    #문제는 피보나치 수열을 재귀가 아닌 반복을 이용하여 빠르게 실행시키는 간단한 문제
int main(){
	int a=1, b=2, c=0,n;
	scanf("%d", &n);
	if (n == 1) c = 1;
	else if (n == 2) c = 2;
	else {
		for (int i = 0; i < n - 2; i++) {
			c = (a + b)%15746;             #그러나 이부분에 c를 지속적으로 작게 유지하지 않아주면 n이 증가함에 따라 c가 너무 커져 오류발생. 여러번 나머지 계산을 해도 나머지 값은 일정!
			a = b;
			b = c;
		}
	}
	printf("%d", c % 15746);
}
