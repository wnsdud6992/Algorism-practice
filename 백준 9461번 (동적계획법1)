#include <stdio.h>                                         #점화식을 찾아서 함수로 표현. 어렵지 않은 문제

long long pado(int num) {                                  ## return값이 굉장히 컸는데 그에 따라 int로는 표현불가. long이나 더긴 long long을 이용하여 수를 8바이트에 저장 
	long long a = 1, b = 1, c = 1,d=0;
	if (num == 1|| num==2 || num==3) return 1;
	else {
		for (int i = 0; i < num - 3; i++) {
			d = a + b;
			a = b;
			b = c;
			c = d;
		}
		return d;
	}
}

int main() {
	int n, a;
	scanf("%d", &n);
	for (int i = 0; i < n; i++) {
		scanf("%d", &a);
		printf("%lld\n",pado(a));                              ## long long형 10진수 정수를 반환하므로 %lld로 받는다.
	}
}
