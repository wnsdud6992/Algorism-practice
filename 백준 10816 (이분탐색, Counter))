from collections import Counter          #파이썬 내장 함수 Counter이용하기 위해 collections 라이브러리 호출

def bi_search(num, arr):                 ## 이분탐색 함수- arr배열에 num존재시 1반환 없으면 0을 반환
    low=0                                
    i=j=1
    high=len(arr)-1
    while low<=high:
        mid=(low+high)//2
        if num == arr[mid]: return 1
        elif num <arr[mid]: high=mid-1
        elif arr[mid] < num: low=mid+1
    return 0


n=int(input())  
arr1=list(map(int,input().split()))
arr1.sort()                              #정렬된 배열에서만 이분탐색이 가능  sort함수는 반환값존재x, 단지 앞에 오는 배열을 정렬시켜준다.
m=int(input())
arr2=list(map(int,input().split()))
counter=Counter(arr1)                    #collections에서 Counter이용하면 각 요소들의 갯수를 dictionary 형태로 저장함. 요소개수 찾을 시 용이

for i in range(m) :
    a=bi_search(arr2[i],arr1)
    if a==1: print(counter[arr2[i]])
    else: print('0')
